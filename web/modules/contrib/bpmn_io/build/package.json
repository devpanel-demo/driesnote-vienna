{
  "name": "bpmn_io",
  "version": "2.0.0",
  "description": "BPMN-JS modeler for Drupal",
  "main": "app/index.js",
  "scripts": {
    "all": "run-s bundle",
    "dev": "run-s bundle:less watch:js",
    "start": "run-s bundle serve",
    "serve": "sirv public --dev",
    "bundle": "run-s bundle:*",
    "bundle:patch": "echo 'patching' && sed -i 's/create.end/off.create.end/' node_modules/bpmn-js/lib/features/label-editing/LabelEditingProvider.js && sed -i 's/autoPlace.end/off.autoPlace.end/' node_modules/bpmn-js/lib/features/label-editing/LabelEditingProvider.js",
    "offbundle:less": "lessc src/app.less public/app.css",
    "bundle:js": "webpack -c webpack.config.js",
    "watch:js": "run-s \"bundle:js -- --watch\""
  },
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/a11y": "^0.1.0",
    "@bpmn-io/element-template-chooser": "^1.1.1",
    "@bpmn-io/element-templates-validator": "^2.6.0",
    "@bpmn-io/properties-panel": "^3.30.1",
    "@camunda/element-templates-json-schema": "^0.12.1",
    "bpmn-auto-layout": "^1.0.1",
    "bpmn-js": "^18.6.2",
    "bpmn-js-color-picker": "^0.7.1",
    "bpmn-js-element-templates": "^2.7.0",
    "bpmn-js-properties-panel": "^5.38.1",
    "bpmn-moddle": "^9.0.2",
    "camunda-bpmn-moddle": "^7.0.1",
    "diagram-js": "^15.3.0",
    "diagram-js-minimap": "^5.2.0",
    "jquery": "^3.7.1",
    "min-dash": "^4.2.3"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^7.0.0",
    "less": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^1.0.14",
    "webpack": "^5.100.1",
    "webpack-cli": "^4.10.0"
  }
}
